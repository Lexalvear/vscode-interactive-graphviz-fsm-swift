{
    "name": "graphviz-interactive-preview",
    "displayName": "Graphviz Interactive Preview",
    "description": "Graphviz (dot) Interactive Preview",
    "version": "0.0.6",
    "keywords": [
        "dot",
        "graphviz",
        "preview",
        "interactive"
    ],
    "publisher": "tintinweb",
    "icon": "images/icon.png",
    "engines": {
        "vscode": "^1.20.2"
    },
    "categories": [
        "Programming Languages"
    ],
    "bugs": {
        "url": "https://github.com/tintinweb/vscode-interactive-graphviz/issues"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/tintinweb/vscode-interactive-graphviz"
    },
    "main": "./src/extension.js",
    "activationEvents": [
        "onCommand:interactive-graphviz.preview.beside"
    ],
    "contributes": {
        "commands": [
            {
                "command": "interactive-graphviz.preview.beside",
                "title": "preview (beside)",
                "category": "Graphviz Interactive"
            }
        ],
        "configuration": {
          "title": "Graphviz Interactive Preview",
          "properties": {
            "interactive-graphviz.minRenderInterval": {
              "type": "number",
              "default": 0,
              "description": "Minimal delay between attempts to auto-render"
            },
            "interactive-graphviz.renderLock": {
              "type": "boolean",
              "default": true,
              "description": "Prevent another rendering to start while rendering is already in process."
            }
          }
        }
    },
    "dependencies": {}
}
